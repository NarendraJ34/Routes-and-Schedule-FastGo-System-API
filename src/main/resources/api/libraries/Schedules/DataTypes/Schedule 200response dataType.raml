#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime:
      type: string
      required: true
      example: "2019-04-30T16:41:41"
    FromLocation:
      type: string
      required: true
      example: "BEN-SG"
    ToLocation:
      type: string
      required: true
      example: "KEL-My"
    availableSeats:
      type: number
      required: true
      example: 50